{
  "name": "vue-base-project",
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "stg": "vue-cli-service build --mode staging & npm run serve & npm --prefix functions run serve",
    "build": "vue-cli-service build --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:stg": "vue-cli-service build --mode staging",
    "serve": "firebase serve --only hosting --port=5000",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "testcafe chrome:headless tests/e2e/**/*.test.ts -S -s screenshots --hostname localhost",
    "docs": "postcss --config build/postcss.config.js docs/book.pcss -o docs/book.css && cd docs && ../node_modules/.bin/gitbook install && ../node_modules/.bin/gitbook serve",
    "lint": "./node_modules/web-base-lib/tools/lint.sh",
    "deploy": "firebase deploy --only hosting,firestore,storage"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.1",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "animate.css": "^3.7.2",
    "apollo-boost": "^0.4.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "axios": "^0.19.0",
    "firebase": "^6.2.4",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "quasar": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.6.2",
    "unfetch": "^4.1.0",
    "urijs": "^1.19.1",
    "validator": "^11.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.14.1",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "web-base-lib": "https://github.com/masterpg/web-base-lib.git"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@types/jest": "~23.3.14",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.10",
    "@types/urijs": "^1.19.3",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/cli-plugin-babel": "~3.11.0",
    "@vue/cli-plugin-eslint": "~3.11.0",
    "@vue/cli-plugin-pwa": "~3.11.0",
    "@vue/cli-plugin-typescript": "~3.11.0",
    "@vue/cli-plugin-unit-jest": "~3.11.0",
    "@vue/cli-service": "~3.11.0",
    "@vue/eslint-config-prettier": "~5.0.0",
    "@vue/eslint-config-typescript": "~4.0.0",
    "@vue/test-utils": "~1.0.0-beta.29",
    "babel-core": "^7.0.0-beta.3",
    "babel-plugin-transform-imports": "^1.5.1",
    "browser-sync": "^2.26.7",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "firebase-admin": "^8.3.0",
    "gitbook-cli": "^2.3.2",
    "prettier": "^1.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "testcafe": "^1.2.1",
    "testcafe-vue-selectors": "^3.1.0",
    "testdouble": "^3.12.0",
